# Paramify Evidence Manager Configuration
# Copy this file to .env and fill in your credentials

# Workspace name (displayed in the app header)
PARAMIFY_WORKSPACE_NAME=My Workspace

# Paramify API URL
# Production: https://app.paramify.com/api/v0
# Staging: https://stage.paramify.com/api/v0
PARAMIFY_API_URL=https://app.paramify.com/api/v0

# Your Paramify API Key
PARAMIFY_API_KEY=your_api_key_here
